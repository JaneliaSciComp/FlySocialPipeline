profiles {

    local {
    }

    lsf {
        process.executor = 'lsf'
        process {
            clusterOptions = '-P scicompsoft'
        }
    }

    docker {
        docker { enabled = true }
        singularity { enabled = false }
    }
}

singularity {
    enabled = true
    autoMounts = true
    cacheDir = "$HOME/.singularity_cache"
}

